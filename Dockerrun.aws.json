{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "moemoo5588/client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "moemoo5588/server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "moemoo5588/worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "moemoo5588/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
            "containerPort": 80,
            "hostPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}